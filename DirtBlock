def block(x,y):
    for i in range(20):
        r1(x+i*3,y,colors01[i])
        r2(x+i*3,y,colors02[i])
        r3(x+i*3,y,colors03[i])
        r4(x+i*3,y,colors04[i])
        r5(x+i*3,y,colors05[i])
        r6(x+i*3,y,colors06[i])
        r7(x+i*3,y,colors07[i])
        r8(x+i*3,y,colors08[i])
        r9(x+i*3,y,colors09[i])
        r10(x+i*3,y,colors10[i])
        r11(x+i*3,y,colors11[i])
        r12(x+i*3,y,colors12[i])
        r13(x+i*3,y,colors13[i])
        r14(x+i*3,y,colors14[i])
        r15(x+i*3,y,colors15[i])
        r16(x+i*3,y,colors16[i])
        r17(x+i*3,y,colors17[i])
        r18(x+i*3,y,colors18[i])
        r19(x+i*3,y,colors19[i])
        r20(x+i*3,y,colors20[i])
