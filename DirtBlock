def r1(x,y,color):
    canvas.create_rectangle(x,y,x+3,y+3,fill=color,outline='')
def r2(x,y,color):
    canvas.create_rectangle(x,y+3,x+3,y+6,fill=color,outline='')
def r3(x,y,color):
    canvas.create_rectangle(x,y+6,x+3,y+9,fill=color,outline='')
def r4(x,y,color):
    canvas.create_rectangle(x,y+9,x+3,y+12,fill=color,outline='')
def r5(x,y,color):
    canvas.create_rectangle(x,y+12,x+3,y+15,fill=color,outline='')
def r6(x,y,color):
    canvas.create_rectangle(x,y+15,x+3,y+18,fill=color,outline='')
def r7(x,y,color):
    canvas.create_rectangle(x,y+18,x+3,y+21,fill=color,outline='')
def r8(x,y,color):
